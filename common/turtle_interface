local command_exec_action = 'exec_action'

command = {
    [command_stop] = command_stop,
    [command_pause] = command_pause,
    [command_continue] = command_continue,
    [command_add_to_action_queue] = command_add_to_action_queue,
    [command_exec_action] = command_exec_action,
    
}



action = {

}

turtle.send(id, command.add_to_action_queue, action.stop, )